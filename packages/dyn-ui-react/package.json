{
  "name": "@dyn-ui/react",
  "version": "0.1.0",
  "description": "React components for DYN UI design system",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "build:vite": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:coverage": "jest --config jest.config.ts --coverage",
    "type-check": "tsc --noEmit",
    "lint:types": "tsc --noEmit --strict",
    "size-check": "npm run build && du -sh dist/* || echo 'Size check complete'"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/classnames": "^2.3.4",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^27.0.0",
    "rollup": "^3.29.5",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.69.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mgasic/dyn-ui.git",
    "directory": "packages/dyn-ui-react"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "design-system",
    "typescript",
    "dyn-ui"
  ],
  "author": "Milos Gasic",
  "license": "MIT"
}