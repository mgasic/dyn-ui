{
  "name": "@dyn-ui/react",
  "version": "0.1.0",
  "description": "React components for DYN UI design system",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:vite": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint \"src/components/**/index.ts\"",
    "test": "vitest run --config ../../vitest.config.ts",
    "test:coverage": "vitest run --coverage --config ../../vitest.config.ts",
    "test:watch": "vitest --config ../../vitest.config.ts",
    "size-check": "npm run build && du -sh dist/* || echo 'Size check complete'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/jest-dom": "^6.1.4",
    "axe-core": "^4.11.0",
    "jest-axe": "^8.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "happy-dom": "^12.10.3",
    "postcss": "^8.4.31",
    "postcss-nested": "^6.0.1",
    "rollup": "^3.29.5",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mgasic/dyn-ui.git",
    "directory": "packages/dyn-ui-react"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "design-system",
    "typescript",
    "dyn-ui"
  ],
  "author": "Mladen Gasic",
  "license": "MIT"
}